SELECT CustomerName,Cast (TotalCost AS varchar) AS TotalCostString FROM CarRentalss
SELECT* FROM CarRentalss WHERE RentalDate < GETDATE()
SELECT '2025-01-01' AS StartDate ,DATEADD (DAY, 5 ,'2025-01-01') AS PredictedReturnDate
SELECT CustomerName ,DATEDIFF(DAY,RentalDate ,ReturnDate) AS RentDuration FROM CarRentalss
