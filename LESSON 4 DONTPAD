CREATE TABLE CarRentalss (
    RentalID INT PRIMARY KEY,
    CustomerName VARCHAR(100),
    CarBrand VARCHAR(50),
    CarModel VARCHAR(50),
    CarType VARCHAR(20),
    FuelType VARCHAR(20),
    Transmission VARCHAR(20),
    RentalDate DATE,
    ReturnDate DATE,
    RentalDays INT,
    DailyRate DECIMAL(7,2),
    TotalCost DECIMAL(10,2),
    
    Rating INT,
    OfficeLocation VARCHAR(100)
);

INSERT INTO CarRentalss (
